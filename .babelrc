{
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-class-properties",
    "@babel/plugin-transform-runtime",
    [
      "module-resolver",
      {
        "root": [
          "./src"
        ],
        "alias": [
          {
            "@components": "./src/components",
            "@themes": "./src/styles/themes",
            "@styles": "./src/styles",
            "@hooks": "./src/hooks"
          }
        ],
        "extensions": [
          ".js",
          ".jsx",
          ".ts",
          ".tsx"
        ]
      }
    ]
  ]
}
